    "/markets/{id}": {
      "get": {
        "description": "Retrieves details of a specific market including its tokens",
        "operationId": "MarketsController_getMarket",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "description": "Market ID",
            "schema": {
              "example": 1,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Market retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MarketDetailResponseDto"
