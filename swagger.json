    },
    "/health": {
      "get": {
        "operationId": "AppController_getHealth",
        "parameters": [],
        "responses": {
          "200": {
            "description": "All services are healthy"
          },
          "503": {
            "description": "One or more services are unhealthy"
          }
        },
        "summary": "Health check endpoint",
        "tags": [
          "system"
        ]
      }
    },
    "/events": {
      "get": {
        "description": "Retrieves a paginated list of synced events with optional filters",
        "operationId": "EventsController_getEvents",
        "parameters": [
          {
            "name": "active",
            "required": false,
            "in": "query",
            "description": "Filter by active status",
            "schema": {
              "example": true,
              "type": "boolean"
            }
          },
          {
            "name": "search",
            "required": false,
            "in": "query",
            "description": "Search by title",
            "schema": {
              "example": "Bitcoin",
              "type": "string"
            }
          },
          {
            "name": "page",
            "required": false,
            "in": "query",
            "description": "Page number (1-indexed)",
            "schema": {
              "default": 1,
              "example": 1,
              "type": "number"
            }
          },
          {
            "name": "pageSize",
            "required": false,
            "in": "query",
            "description": "Number of items per page",
            "schema": {
              "default": 20,
              "example": 20,
              "type": "number"
            }
          },
          {
            "name": "limit",
            "required": false,
            "in": "query",
            "description": "Number of items per page (alias for pageSize)",
            "schema": {
              "default": 20,
              "example": 20,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Events retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EventListResponseDto"
                }
              }
            }
          }
        },
        "summary": "List events",
        "tags": [
          "events"
        ]
      }
    },
    "/events/{id}": {
      "get": {
        "description": "Retrieves details of a specific event including its markets",
        "operationId": "EventsController_getEvent",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "description": "Event ID",
            "schema": {
              "example": 1,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Event retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EventDetailResponseDto"
                }
              }
            }
          },
          "404": {
            "description": "Event not found"
          }
        },
        "summary": "Get event by ID",
        "tags": [
          "events"
        ]
      }
    },
    "/events/{eventId}/markets": {
      "get": {
        "description": "Retrieves all markets associated with a specific event",
        "operationId": "EventsController_getEventMarkets",
        "parameters": [
          {
            "name": "eventId",
            "required": true,
            "in": "path",
            "description": "Event ID",
            "schema": {
              "example": 1,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Markets retrieved successfully"
          },
          "404": {
            "description": "Event not found"
          }
        },
        "summary": "Get markets for an event",
        "tags": [
          "events"
        ]
      }
    },
    "/events/sync": {
      "post": {
        "description": "Triggers a manual sync to fetch and update events and markets from Polymarket",
        "operationId": "EventsController_syncEvents",
        "parameters": [
          {
            "name": "limit",
            "required": false,
            "in": "query",
            "description": "Maximum number of events to sync",
            "schema": {
              "example": 100,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Sync completed successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SyncResponseDto"
                }
              }
            }
          }
        },
        "summary": "Sync events from Polymarket",
        "tags": [
          "events"
        ]
      }
    },
